{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "aginev/login-activity": "1.0.*",
        "arcanedev/seo-helper": "1.4.*",
        "aws/aws-sdk-php": "^3.64",
        "creativeorange/gravatar": "~1.0",
        "davejamesmiller/laravel-breadcrumbs": "4.x",
        "dingo/api": "2.0.0-alpha2",
        "drewm/mailchimp-api": "^2.5",
        "fideloper/proxy": "~3.3",
        "greggilbert/recaptcha": "dev-master",
        "gufy/cpanel-whm": "dev-master",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "kim/activity": "^1.1",
        "lahaxearnaud/laravel-pushbullet": "^1.1",
        "laramanager/api-tester": "dev-master",
        "laramanager/config": "dev-master",
        "laramanager/helpers": "dev-master",
        "laramanager/laramanager": "^1.0",
        "laramanager/media-manager": "dev-master",
        "laramanager/reporter": "dev-master",
        "laramanager/scheduling": "dev-master",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.1",
        "laravel/tinker": "~1.0",
        "laravelium/sitemap": "2.7.*",
        "maatwebsite/excel": "~2.1.0",
        "nesbot/carbon": "^1.32",
        "parsedown/laravel": "^1.1",
        "pragmarx/firewall": "^2.2",
        "rap2hpoutre/laravel-credit-card-validator": "^0.0.6",
        "renatomarinho/laravel-page-speed": "^1.8",
        "riverskies/laravel-mobile-detect": "^1.3",
        "s-ichikawa/laravel-sendgrid-driver": "^2.0",
        "sendgrid/sendgrid": "^7.2",
        "spatie/eloquent-sortable": "^3.4",
        "spatie/laravel-analytics": "^3.6",
        "spatie/laravel-newsletter": "^4.2",
        "tymon/jwt-auth": "1.0.x-dev"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
